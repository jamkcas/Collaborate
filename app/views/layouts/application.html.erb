<!DOCTYPE html>
<html>
<head>
  <title>Collaborate</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= flash[:errors] if flash[:errors] %>
<% if current_user %>
  <%= link_to 'Logout', logout_path %>
  <%= link_to 'Change_Password', edit_user_path %>
<% end %>

<%= yield %>

</body>
</html>
